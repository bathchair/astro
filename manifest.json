{
    "name": "Astro",
    "description": "Analyze any date and find the astrology sign associated.",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "16": "icons/astro16.png",
        "48": "icons/astro48.png",
        "128": "icons/astro128.png"
    },

    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Astro"
    },

    "permissions": [
        "activeTab",
        "tabs"
    ]
}